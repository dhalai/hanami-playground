<% if current_user %>
  <% if current_user.admin? %>
    <div class="menu">
      <%=link_to "Home", routes.root_path %>
      <%=link_to "Users", routes.users_path %>
    </div>
  <% end %>

  <%=
    form_for :session, routes.session_path, method: :delete do
      submit 'Sign out'
    end
  %>
<% end %>
